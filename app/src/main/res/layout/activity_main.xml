<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:background="@drawable/tree">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:id="@+id/scoreText"
        android:textSize="40dp"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/imageView7" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="残り時間０"
        android:id="@+id/timeText"
        android:textSize="20dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView9"
        android:layout_marginLeft="100dp"
        android:layout_marginRight="10dp"
        android:src="@drawable/zyoushi1"
        android:layout_below="@+id/scoreText"
        android:layout_alignStart="@+id/imageView3"
        android:layout_marginTop="200dp"
        android:onClick="tapMole"
        android:tag="8" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView"
        android:src="@drawable/zyoushi1"
        android:layout_marginLeft="10dp"
        android:onClick="tapMole"
        android:tag="0"
        android:layout_below="@+id/imageView7" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView2"
        android:layout_marginTop="65dp"
        android:src="@drawable/zyoushi1"
        android:layout_below="@+id/imageView"
        android:layout_toStartOf="@+id/imageView"
        android:onClick="tapMole"
        android:tag="1" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView3"
        android:src="@drawable/zyoushi1"
        android:layout_alignTop="@+id/imageView"
        android:layout_toStartOf="@+id/imageView2"
        android:onClick="tapMole"
        android:tag="2" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView4"
        android:layout_alignTop="@+id/imageView2"
        android:layout_alignEnd="@+id/imageView"
        android:src="@drawable/zyoushi1"
        android:layout_marginTop="-200dp"
        android:onClick="tapMole"
        android:tag="3" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView5"
        android:layout_below="@+id/imageView2"
        android:layout_alignParentStart="true"
        android:src="@drawable/zyoushi1"
        android:layout_marginTop="-300dp"
        android:onClick="tapMole"
        android:tag="4" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView6"
        android:src="@drawable/zyoushi1"
        android:layout_alignTop="@+id/imageView4"
        android:layout_alignStart="@+id/imageView3"
        android:onClick="tapMole"
        android:tag="5" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView7"
        android:src="@drawable/zyoushi1"
        android:onClick="tapMole"
        android:tag="6"
        android:layout_above="@+id/imageView12"
        android:layout_toEndOf="@+id/imageView11"
        android:layout_marginBottom="62dp" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView8"
        android:src="@drawable/zyoushi1"
        android:onClick="tapMole"
        android:tag="7"
        android:layout_above="@+id/imageView4"
        android:layout_toEndOf="@+id/scoreText" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView10"
        android:layout_below="@+id/imageView2"
        android:layout_toEndOf="@+id/textView"
        android:tag="9"
        android:src="@drawable/cat1" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView11"
        android:layout_below="@+id/imageView7"
        android:tag="10"
        android:src="@drawable/cat1" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView12"
        android:layout_centerVertical="true"
        android:layout_alignStart="@+id/imageView9"
        android:src="@drawable/cat1"
        android:tag="11" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Start"

        android:onClick="start"
        android:layout_alignBottom="@+id/scoreText"
        android:layout_centerHorizontal="true" />

</RelativeLayout>

